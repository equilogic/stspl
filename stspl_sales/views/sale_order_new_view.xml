<openerp>
	<data>
		<record id="inherited_sale_order_form" model="ir.ui.view">
            <field name="name">sale.order.inherited.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@string='Other Information']//field[@name='company_id']" position='after'>
                    <field name="ship_via_id"/>
                </xpath>
         	</field>
        </record>
	</data>
</openerp>